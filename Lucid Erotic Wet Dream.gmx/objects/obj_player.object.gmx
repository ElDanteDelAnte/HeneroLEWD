    time += 1;
    if(act == "Escape")
    {
        //Dante: Is this a local variable?
        lastImage = image_number;
        if(image_index &gt;= lastImage - 4)
        {
            image_speed = 0;
        }
    }
    
    if(time &gt;= timer)
    {
        resume();
    }
}


